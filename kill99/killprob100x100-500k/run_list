400..449 hdiv2_divmut001_500k_killprob avida -s $seed -set DIV_MUT_PROB 0.001 -set WORLD_X 100 -set WORLD_Y 100 -set EVENT_FILE events-100k-hdiv2.cfg -def INST_SET instset-heads-hdiv2.cfg
500..549 hdiv3_divmut001_500k_killprob avida -s $seed -set DIV_MUT_PROB 0.001 -set WORLD_X 100 -set WORLD_Y 100 -set EVENT_FILE events-100k-hdiv3.cfg -def INST_SET instset-heads-hdiv3.cfg
